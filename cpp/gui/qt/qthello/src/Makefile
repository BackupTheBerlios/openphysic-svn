dist: 
	@mkdir -p .tmp/qthello && $(COPY_FILE) --parents $(SOURCES) $(HEADERS) $(FORMS) $(DIST) .tmp/qthello/ && ( cd `dirname .tmp/qthello` && $(TAR) qthello.tar qthello && $(GZIP) qthello.tar ) && $(MOVE) `dirname .tmp/qthello`/qthello.tar.gz . && $(DEL_FILE) -r .tmp/qthello


\newcommand{\noteadmin}[9]{%
  % % #1 : Année 1 - Année 2
  % % #2 : Corps (professeur agrégé)
  % % #3 : Echelon
  % % #4 : Classe
  % % #5 : Note proposée
  % % #6 : Note péréquée
  % % #7 : Grille 1
  % % #8 : Grille 2
  % % #9 : Grille 3
  \addtocounter{cntadmin}{1}
  \rput(0,-\value{cntadmin}){%
    \grillereference{#7}{#8}{#9}
    \noteproposee(#5)
    \noteperequee(#6)
    % affichage (année, corps, classe, échelon)
    \rput(31,0.25){\tiny #1}
    \rput(31,0){\tiny #2 #3\ieme echelon}
    \rput(31,-0.25){\tiny #4}
  }
}


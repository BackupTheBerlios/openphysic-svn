\newcommand{\grillenotationpedago}[1]{%
\noindent
\begin{pspicture}(15,-0.5)(30,0.5)
%\psgrid[subgriddiv=1,griddots=10]
\psset{xunit=0.5}
\psset{yunit=1}
\psline(30,0)(60,0)
\multido{\n=30+1}{31}{%
\psline(\n,-0.1)(\n,0.1)
}
\multido{\n=30+2}{16}{%
\rput(\n,0.25){$\n$}
}
\multido{\n=30+2}{15}{%
\psline[linewidth=0.1pt](\n,0)(\n,-16)
\rput(1,0){\psline[linewidth=0.1pt,linestyle=dashed](\n,0)(\n,-16)}
}
\psline[linewidth=0.1pt](60,0)(60,-16)
#1
\end{pspicture}
}



\newcommand{\notepedago}[8]{%
  \addtocounter{cntpedago}{1}
  % #1 : Date
  % #2 : Corps (professeur agrégé)
  % #3 : Echelon
  % #4 : Classe
  % #5 : Note
  % #6 : Grille 1
  % #7 : Grille 2
  % #8 : Grille 3
  % note
  \rput(0,-\value{cntpedago}){%
    \pscircle(#5,0){0.1}
    \rput(#5,0.25){$#5$}
    \grillereference{#6}{#7}{#8}
    % affichage (année, corps, classe, échelon)
    \rput(28,0.25){\tiny #1}
    \rput(28,0){\tiny #2 #3\ieme echelon}
    \rput(28,-0.25){\tiny #4}
  }
}


\newcommand{\noteinspection}[8]{%
  \addtocounter{cntpedago}{1}
  % #1 : Date
  % #2 : Corps (professeur agrégé)
  % #3 : Echelon
  % #4 : Classe
  % #5 : Note
  % #6 : Grille 1
  % #7 : Grille 2
  % #8 : Grille 3
  \rput(0,-\value{cntpedago}){%
    \pscircle*(#5,0){0.1}
    \rput(#5,0.25){$#5$}
    % affichage (année, corps, classe, échelon)
    \rput(28,0.25){\tiny #1}
    \rput(28,0){\tiny #2 #3\ieme echelon}
    \rput(28,-0.25){\tiny #4}
    \grillereference{#6}{#7}{#8}
  }
}


